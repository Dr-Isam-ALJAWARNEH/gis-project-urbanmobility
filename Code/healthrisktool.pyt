import arcpyfrom arcpy.sa import *class Toolbox(object):    def __init__(self):        self.label = "Health Risk Mapping Toolbox"        self.alias = "healthrisk"        self.tools = [HealthRiskTool]class HealthRiskTool(object):    def __init__(self):        self.label = "Generate Final Health Risk Maps"        self.description = "Reclassifies population, industry and roads and creates weighted health risk maps."    def getParameterInfo(self):        return []    def isLicensed(self):        return True    def updateParameters(self, parameters):        return    def updateMessages(self, parameters):        return    def execute(self, parameters, messages):        gdb = r"C:\Users\SAR\AppData\Local\Microsoft\Olk\Attachments\ooa-535e303e-d358-454a-a863-18f94e7347dd\6e3c5528e453b52f06870e2f9d3c2647dc4fcf8f2317b7fae593b652e71f4334\dr nezar.gdb"        arcpy.env.workspace = gdb        arcpy.env.overwriteOutput = True        arcpy.CheckOutExtension("Spatial")        pop_raster_name = "popdensity"        industry_raster_name = "landuse_ras"        roads_raster_name = "roads_ras"        pop_in = Raster(pop_raster_name)        industry_in = Raster(industry_raster_name)        roads_in = Raster(roads_raster_name)        pop = Reclassify(pop_in, "Value",                         RemapRange([[0, 100, 1],                                     [100, 300, 2],                                     [300, 99999, 3]]))        pop.save("population_density")        industry = Reclassify(industry_in, "Value",                              RemapRange([[1, 1, 1],                                          [2, 2, 2],                                          [3, 3, 3]]))        industry.save("industrial_activity")        roads = Reclassify(roads_in, "Value",                           RemapRange([[0, 0.0001, 1],                                       [0.0001, 0.001, 2],                                       [0.001, 9999, 3]]))        roads.save("roads_high_impact")        weighted = (industry * 0.5) + (roads * 0.3) + (pop * 0.2)        weighted.save("weighted_risk_index")        final = Reclassify(weighted, "Value",                           RemapRange([[0, 2, 1],                                       [2, 5, 2]]))        final.save("final_health_risk_levels")        arcpy.AddMessage("Health risk layers created successfully.")